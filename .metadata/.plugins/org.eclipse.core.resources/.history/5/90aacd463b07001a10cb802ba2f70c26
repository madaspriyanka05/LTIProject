package com.lti.model;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.JoinColumn;

@Entity

public class Account
{
	@Id
	private int accId;
	
	@OneToOne
	@JoinColumn(name = "P_ID", nullable=false)// name of the foreign key
	private int custId;
	
	
	
	
	}